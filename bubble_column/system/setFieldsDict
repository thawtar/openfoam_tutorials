FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    cylinderToCell
    {
        point1 (0.05 0.05 -1); 
        point2 (0.05 0.05 1);
        radius 0.01;
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
);


